/* CSS Document */

body{
	margin:0 0 0 0;
	background-color: #f9f9f9;	
	color:#0b7392;
	font-family:  "宋体",Arial, Helvetica, sans-serif;
    font-size: 12px;
    text-align:center;
}
a:link {
	font-family:  "宋体",Arial, Helvetica, sans-serif;
	font-size: 9pt;
	color: #074393;
	text-decoration: none;
}
a:visited {
	font-family:  "宋体",Arial, Helvetica, sans-serif;
	font-size: 9pt;
	color: #074393;
	text-decoration: none;
}

a:hover {
	font-family:  "宋体",Arial, Helvetica, sans-serif;
	font-size: 9pt;
	color: #ab7901;
	text-decoration: underline;
}
.inputclass{

	font-size:12px;
	border:1px  solid #0a8d8b;
	width:120px;
}
